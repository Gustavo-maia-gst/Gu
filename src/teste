struct Pessoa {
    nome: char[12];
    idade: int;
    mae: Pessoa*;

    func falar(this: Pessoa) -> void {

    };
}

func main() -> int {
    var m: Pessoa;
    m.nome = "Edivania";
    m.idade = 40;

    var p: Pessoa;
    p.nome = "Gustavo";
    p.idade = 19;
    p.mae = m;

    print(p.nome[0], p.funcao());

    return;
}